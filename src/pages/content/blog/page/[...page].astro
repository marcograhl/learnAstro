---
import Feed from '$layouts/Feed.astro'
export function getStaticPaths({paginate}){
    let posts = Astro.fetchContent('../../../../posts/*md')
    return paginate( posts,{pageSize:5})
}
const contentArr = Astro.fetchContent('../index.md')

const content = contentArr[0]
const {page} = Astro.props
---

<Feed page={page} content={content} />